<UserControl x:Class="CommsDemo.Views.AlarmMute"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CommsDemo.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="40">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/CommsDemo;component/Resources/Images/BLANK.bmp"/>
        </Grid.Background>
        <Image x:Name="AlarmMuteImg" Source="{Binding AlarmMutePath}" HorizontalAlignment="Left" Height="30" Margin="5,62,-1511,0" VerticalAlignment="Top" Stretch="UniformToFill" Width="35"/>
    </Grid>
</UserControl>
